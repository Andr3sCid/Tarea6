adas()\" :key=\"comidaRecomendada.id\"\n  >\n    <b-carousel-slide\n      caption={{comidaRecomendada.nombre}}\n      img-src={{comidaRecomendada.img}}\n    ></b-carousel-slide>\n  </b-carousel>\n</div>\n\n    <b-row>\n      <b-col v-for=\"comida in comidas\" :key=\"comida.id\">\n        <b-card id=\"card-presentacion\" class=\"shadow p-3 mb-5 bg-body rounded\">\n          <b-img\n            thumbnail\n            fluid\n            id=\"img-carg\"\n            :src=\"`${comida.img}`\"\n            alt=\"imagen-comida\"\n            img-top\n          ></b-img>\n          <b-card-text>\n            <div>\n              {{ comida.nombre }}\n              <div>${{ comida.precio }}</div>\n            </div>\n            <div>\n              <button id=\"btn-agregarCarrito\">+</button>\n            </div>\n          </b-card-text>\n        </b-card>\n      </b-col>\n    </b-row>\n  </b-container>\n</template>\n\n<style>\n#card-presentacion {\n  width: 250px;\n  height: 300px;\n  background-color: #a74ac7;\n}\n</style>"]},"metadata":{},"sourceType":"module"}